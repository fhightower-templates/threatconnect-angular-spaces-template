{
  "allowOnDemand": true,
  "languageVersion": "7",
  "params": [{
    "encrypt": false,
    "label": "Api User",
    "name": "api_user",
    "sequence": 1,
    "type": "String",
    "validValues": ["${USER:TEXT}", "${ORGANIZATION:TEXT}"]
  }, {
    "encrypt": true,
    "label": "API Token",
    "name": "api_token",
    "sequence": 2,
    "type": "String",
    "validValues": ["${USER:KEYCHAIN}", "${ORGANIZATION:KEYCHAIN}"]
  }, {
    "default": "info",
    "encrypt": false,
    "label": "Logging Level",
    "name": "logging",
    "sequence": 3,
    "type": "Choice",
    "validValues": [
      "debug",
      "info",
      "warn",
      "error",
      "critical"
    ]
  }],
  "programLanguage": "NONE",
  "programVersion": "{{ cookiecutter.version }}",
  {% if cookiecutter.runtime_context == 'Context' %}"runtimeContext": [
    "Address",
    "EmailAddress",
    "File",
    "Host",
    "Url",
    "Adversary",
    "Campaign",
    "Document",
    "Email",
    "Incident",
    "Signature",
    "Tag",
    "Task",
    "Threat",
    "Victim"
  ],{% elif cookiecutter.runtime_context == 'Menu' %}"runtimeContext": ["Menu"],{% elif cookiecutter.runtime_context == 'Search' %}"runtimeContext": ["Search"],{% else %}"runtimeContext": [],{% endif %}
  "runtimeLevel": "SpaceOrganization"
}
